{
  "expo": {
    "name": "ShredzAI",
    "slug": "shredzai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAF9F6"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.shredzai",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to analyze body composition for accurate body fat percentage calculations.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to save and analyze body composition photos."
      }
    },
    "android": {
    },
    "web": {
      "enabled": false
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow ShredzAI to access your camera to analyze body composition."
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true
    },
    "extra": {
      "supabaseUrl": "https://gufbulaxyqcggaeqqvhh.supabase.co",
      "supabaseAnonKey": "sb_publishable_uqD0kRP_huqdK204EveHyw_64Y9j7KS"
    }
  }
}
